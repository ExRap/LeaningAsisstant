{
  "name": "exraplearningextension",
  "displayName": "ExRapLearningExtension",
  "description": "The greatest learning experice",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "exraplearningextension.pythonTutorial",
        "title": "Start learning Python"
      },
      {
        "command": "exraplearningextension.fullquestionhandler",
        "title": "Answer Question"
      },
      {
        "command": "exraplearningextension.describecodehandler",
        "title": "Describe Code"
      },
      {
        "command": "exraplearningextension.writeFunctionhandler",
        "title": "Write Function"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "genezio": "^0.3.5",
    "http": "^0.0.1-security",
    "https": "^1.0.0"
  }
}
